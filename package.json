{
  "name": "xiaofan",
  "version": "1.10.4",
  "description": "Fanfou WeApp",
  "private": true,
  "scripts": {
    "test": "xo && ava",
    "upload": "cli/upload.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fanfoujs/xiaofan.git"
  },
  "author": "FanfouJS",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fanfoujs/xiaofan/issues"
  },
  "homepage": "https://github.com/fanfoujs/xiaofan#readme",
  "devDependencies": {
    "ava": "^0.25.0",
    "chalk": "^2.4.1",
    "deep-keys": "^0.5.0",
    "execa": "^0.10.0",
    "ora": "^3.0.0",
    "xo": "^0.22.0"
  },
  "xo": {
    "globals": [
      "App",
      "wx",
      "Page",
      "getApp",
      "getCurrentPages"
    ],
    "ignores": [
      "src/fanfou-sdk-weapp/**"
    ],
    "rules": {
      "ava/no-ignored-test-files": 0,
      "camelcase": 0,
      "import/no-unresolved": 0,
      "max-params": [
        "error",
        6
      ],
      "new-cap": [
        "error",
        {
          "capIsNew": false
        }
      ],
      "prefer-rest-params": 0,
      "space-before-function-paren": [
        "error",
        "always"
      ],
      "unicorn/import-index": 0
    },
    "semicolon": false,
    "space": 2
  },
  "dependencies": {}
}
